@article{grigorenko2014struktura7336610,
 address = {Москва},
 annote = {В работе сообщается о результатах многоуровневого моделирования методом квантовой механики/молекулярной механики (КМ/ММ) структуры сложной молекулярной системы – фермент-субстратного комплекса для одного из важнейших природных ферментов. Полноатомная трехмерная модель каталитического EAL домена фосфодиэстераз с включенным в систему субстратом, циклическим димерным гуанозинмонофосфатом, построена по координатам тяжелых атомов одной из доступных структур из базы данных белковых структур (PDB). Далее в приближении КМ/ММ с использованием методов теории функционала плотности для достаточно большой квантовой подсистемы оптимизированы равновесные геометрические параметры модели. Результаты сопоставлены с параметрами двух кристаллографических структур PDB, что позволяет судить о достоверности полученных данных. Анализ расположения молекулярных групп в активном центре фермента и сравнение новых результатов с данными прежних расчетов реакций гидролиза нуклеозидфосфатов подсказывает направление ферментативной реакции раскрытия цикла в субстрате.},
 author = {Григоренко, Б. Л. and Немухин, А. В. and Хренова, М. Г. and Новичкова, Д. А.},
 issn = {0579-9384},
 journal = {Вестник Московского университета. Серия 2: Химия},
 language = {russian},
 number = {1},
 pages = {3--6},
 publisher = {Москва},
 title = {Структура фермент-субстратного комплекса каталитического домена фосфодиэстераз с циклическим дигуанозинмонофосфатом},
 volume = {55},
